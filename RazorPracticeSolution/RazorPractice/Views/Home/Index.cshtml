@{
    var nameList = new List<string>();
    nameList.Add("Veronica");
    nameList.Add("Sam");
    nameList.Add("Max");
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">

    @{
        var UserName = "Mary";
        var Greeting = "Howdy!";
    }
    @Html.Raw("<h3>" + Html.Encode(Greeting) + " " + Html.Encode(UserName) + "</h3>")

    @foreach (var person in nameList)
    {
        <ul>
            @person
        </ul>
    }

    @if (DateTime.Equals(DateTime.Now.DayOfWeek, DayOfWeek.Friday))
    {
        @Html.Raw("TGIF!!")
    }
    else
    {
        @Html.Raw("<p>" + "It's not Friday!" + "</p>")
    }
</div>